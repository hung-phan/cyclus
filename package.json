{
  "name": "cyclus",
  "description": "IOC and lifecycle for javascript object",
  "version": "2.0.0",
  "author": "hung-phan",
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
  },
  "engines": {
    "node": ">= 4.1.0",
    "npm": ">=3.0.0"
  },
  "files": [
    "dist",
    "src",
    "preprocessor.js",
    "README.md",
    "tsconfig.json"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testURL": "http:localhost/",
    "testMatch": [
      "**/__tests__/*.spec.(ts|js)"
    ]
  },
  "jsnext:main": "dist/esnext/index.js",
  "keywords": [
    "dependency injection",
    "inversion of control",
    "lifecycle"
  ],
  "license": "MIT",
  "main": "dist/es6/index.js",
  "module": "dist/esnext/index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "typings": "dist/es6/index.d.ts"
}
